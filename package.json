    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "smui-theme": "^4.2.0",
    "svelte": "^3.41.0"
  },
  "type": "module",
  "dependencies": {
    "@smui/button": "^4.2.0",
    "@smui/data-table": "^4.2.0",
    "@smui/drawer": "^4.2.0",
